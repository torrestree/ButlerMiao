<UserControl
    x:Class="ServerWpf.View.Common.SignIn.ViewSignIn"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:ServerWpf.View.Common.SignIn"
    xmlns:vm="clr-namespace:CoreServer.ViewModel.Common;assembly=CoreServer"
    xmlns:h="clr-namespace:HelperWpf.View.Common;assembly=HelperWpf"
    mc:Ignorable="d" 
    d:DesignHeight="450" d:DesignWidth="800" d:DataContext="{d:DesignInstance Type=vm:VmSignIn}">
    <Grid>
        <local:UcSignIn DataContext="{Binding}"/>
        <h:UcMessage DataContext="{Binding Path=VmMessage}"/>
    </Grid>
</UserControl>
